   "source": [
    "# Demonstrate common slicing operations.\n",
    "\n",
    "seq = list(range(10))\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print(\"Original:\", seq)\n",
    "    print(\"First 3:\", seq[:3])\n",
    "    print(\"Last 3:\", seq[-3:])\n",
    "    print(\"Every 2nd:\", seq[::2])\n",
    "    print(\"Reversed:\", seq[::-1])"
   ]
